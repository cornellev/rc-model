<launch>

  <rosparam file="$(find rc_model)/launch/controller.yaml" command="load" />
  <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
    output="screen" ns="rc_model"
    args="Revolute 304_position_controller Revolute 275_position_controller Revolute 276_position_controller Revolute 277_position_controller Revolute 284_position_controller Revolute 285_position_controller Revolute 286_position_controller Revolute 287_position_controller Revolute 307_position_controller joint_state_controller " />
  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher"
    respawn="false" output="screen">
    <remap from="/joint_states" to="/rc_model/joint_states" />
  </node>

</launch>